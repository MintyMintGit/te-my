@model  _1TE_MY.Models.DTO.Registration_UserInformation
@using Westwind.Globalization
@using Westwind.Globalization.AspNetCore.Extensions


<div class="row">
    <table class="table  table-vmiddle">
        <thead>
            <tr>
                <th width="60">@DbRes.T("SNo", "Registration")</th>
                <th>@DbRes.T("StatusDescription", "Registration")</th>
                <th>@DbRes.T("DateTime", "Registration")</th>
                <th>@DbRes.T("Status", "Registration")</th>
                <th>@DbRes.T("Remark", "Registration")</th>
            </tr>
        </thead>
        <tbody>
            <!-- ngRepeat: item in registrationStatusList -->
            <tr ng-repeat="item in registrationStatusList" class="ng-scope" style="">
                <td >1</td>
                <td >Awaiting Verification</td>
                <td >13/11/2017 11:41 PM</td>
                <td><input type="checkbox" /></td>
                <td>Completed</td>
            </tr><!-- end ngRepeat: item in registrationStatusList -->
            <tr ng-repeat="item in registrationStatusList" class="ng-scope">
                <td >2</td>
                <td >Email Verified</td>
                <td >13/11/2017 11:41 PM</td>
                <td><input type="checkbox" /></td>
                <td>Pending</td>
            </tr><!-- end ngRepeat: item in registrationStatusList -->
            <tr ng-repeat="item in registrationStatusList" class="ng-scope">
                <td >3</td>
                <td >Draft Submitted</td>
                <td >13/11/2017 11:40 PM</td>
                <td><input type="checkbox" /></td>
                <td>Pending</td>
            </tr><!-- end ngRepeat: item in registrationStatusList -->
            <tr ng-repeat="item in registrationStatusList" class="ng-scope">
                <td >4</td>
                <td >Awaiting Email Verification</td>
                <td >13/11/2017 11:40 PM</td>
                <td><input type="checkbox" checked="checked" /></td>
                <td>Pending</td>
            </tr><!-- end ngRepeat: item in registrationStatusList -->
        </tbody>
    </table>
    
</div>
 
